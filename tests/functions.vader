Given fsharp:
  let func =
    match something with
    | First case -> something
    | Second case ->
      doSomething
    | Third case ->
      doOtherThing

Do:
  Go|

Execute (ScopedFind):
  let actual = ScopedFind('^\s*match', '^\s*let\s.*=$', line('$'), indent(line('$')))
  AssertEqual 2, actual

Given fsharp:

Do:
  Go\<Esc>

Execute (TrimSpacesAndComments):
  let line = '  let someFunc = // This should be ignored'
  let actual = TrimSpacesAndComments(line)
  AssertEqual 'let someFunc =', actual

